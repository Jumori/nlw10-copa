{
  "env": {
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "standard-with-typescript",
    "plugin:prettier/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": ["react", "prettier"],
  "globals": {
    "__DEV__": "readonly"
  },
  "rules": {
    "@typescript-eslint/strict-boolean-expressions": "off"
  }
}
